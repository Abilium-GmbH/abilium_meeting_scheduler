<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="group_scheduler_timeform" model="ir.ui.view">
            <field name="name">group_scheduler.timeform</field>
            <field name="model">group_wizard</field>
            <field name="arch" type="xml">
                <form string="Test">
                    <sheet>
                        <group>
                            <group>
                                <!--                                <field name="name"/>-->
                                <field name="search_start_date"/>
                                <field name="search_end_date"/>
                                <field name="working_hour_start" widget="float_time"/>
                                <field name="working_hour_end" widget="float_time"/>


                            </group>
                            <group>
                                <!--                                <field name="last_seen"/>-->
                                <button name="transit_button_timeslots_from_intersection"
                                        string="V1 Generate timeslots from Intersection"
                                        class="btn-primary" type="object"/>
                                <button name="transit_button_timeslots_from_union"
                                        string="V2 Generate timeslots from Union"
                                        class="btn-primary" type="object"/>

                            </group>
                            <notebook>
<!--                                <page string="Description">-->
<!--                                    &lt;!&ndash;                                    <field name="description"/>&ndash;&gt;-->


<!--                                </page>-->
                            </notebook>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_group_scheduler_timeform" model="ir.actions.act_window">
            <field name="name">Add Language</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">group_wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>




    </data>
</odoo>




